package arkadasSayi;

public class Main {

	public static void main(String[] args) {
		//arkadasSayi
		
				int number1 = 220;
				int number2 = 284;
				int total1=0;
				int total2=0;
				
				
				for(int j=0; j<number1; j++) {
					if(number1 % j==0) {
					   total1=total1+j;
					}
				}
				for(int k=0; k<number2; k++) {
					if(number2%k==0) {
						total2=total2+k;
					}
				}
				
				if(number1==total2 && number2==total1) {
					System.out.println("Arkadas Sayi");
				}else {
					System.out.println("Arkadas sayilar degil");
				}
				
				
				
	}

}
