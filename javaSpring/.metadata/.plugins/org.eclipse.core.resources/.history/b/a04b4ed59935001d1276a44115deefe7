package arkadasSayi;

public class Main {

	public static void main(String[] args) {
		//arkadasSayi
		
		int number1= 220;
		int number2= 284;
		int total1=0;
		int total2=0;
		
		for(int i=0; i<number1; i++) {
			if(number1==total1) {
				total1=total1+i;
			}
		}
		for(int j=0; j<number2; j++) {
			if(number2==total2) {
				total2=total2+j;
			}
		}
		
		if(number1==total2 && number2==total1) {
			System.out.println("Arkadas Sayilar");
		}else {
			System.out.println("Arkadas Sayilar Degil");
		}
				
				
	}

}
