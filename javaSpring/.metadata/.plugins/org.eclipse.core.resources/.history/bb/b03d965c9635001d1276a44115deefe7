package helloWorld;

public class Main {

	public static void main(String[] args) {
		//helloWorld
		System.out.println("Hello World");

		
		
		
		
		//arkadasSayi
		
		int sayi1 = 220;
		int sayi2 = 284;
		int toplam1=0;
		int toplam2=0;
		
		
		for(int j=0; j<sayi1; j++) {
			if(sayi1%j==0) {
			   toplam1=toplam1+j;
			}
		}
		for(int k=0; k<sayi2; k++) {
			if(sayi2%k==0) {
				toplam2=toplam2+k;
			}
		}
		
		if(sayi1==toplam2 && sayi2==toplam1) {
			System.out.println("Arkadas Sayi");
		}else {
			System.out.println("Arkadas sayilar degil");
		}
		
		
		
		
		
		


}
	
}